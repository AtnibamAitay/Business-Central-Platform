<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="space.atnibam.pms.mapper.ProductsMapper">

    <resultMap id="BaseResultMap" type="space.atnibam.pms.model.entity.Products">
        <id property="productId" column="product_id" jdbcType="INTEGER"/>
        <result property="coverImage" column="cover_image" jdbcType="VARCHAR"/>
        <result property="merchantId" column="merchant_id" jdbcType="INTEGER"/>
        <result property="productName" column="product_name" jdbcType="VARCHAR"/>
        <result property="price" column="price" jdbcType="DECIMAL"/>
        <result property="orderCount" column="order_count" jdbcType="INTEGER"/>
        <result property="inventory" column="inventory" jdbcType="INTEGER"/>
        <result property="creationTime" column="creation_time" jdbcType="TIMESTAMP"/>
        <result property="statusCode" column="status_code" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        product_id,cover_image,merchant_id,
        product_name,price,order_count,
        inventory,creation_time,status_code
    </sql>
</mapper>
